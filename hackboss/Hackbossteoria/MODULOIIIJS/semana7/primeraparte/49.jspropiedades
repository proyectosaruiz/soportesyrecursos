console.log(window); //SISTEMA OPERATIVO

console.log("innertHeigth", window); //ALTURA

console.log("innerHeight", window.innerHeight);

console.log("innertwidth", window.innerWidth); //ANCHO

console.log("LOCATION", window.location); //LOCACION

